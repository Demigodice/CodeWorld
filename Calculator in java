import java.util.*;
public class Calculator {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int n2=sc.nextInt();
        System.out.println("Enter 1 for ADD\nEnter 2 for SUB\nEnter 3 for MUL\nEnter 4 for DIV\nEnter 5 for Remainder");
        int option=sc.nextInt();
        switch(option){
            case 1:
            System.out.println(n1+n2);
            break;
            case 2:
            System.out.println(n1-n2);
            break;
            case 3:
            System.out.println(n1*n2);
            break;
            case 4:
            System.out.println(n1/n2);
            break;
            case 5:
            System.out.println(n1%n2);
            break;
            default:
            System.out.println("INVALID CHOICE");



        }
    }
    
}
